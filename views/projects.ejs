<!DOCTYPE html>
<html>

<head>
    <title>Intents</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css"
        integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
</head>

<body>



    <div class="container">
        <div class="mb-3">
            <label for="upload" class="form-label">Upload Agent</label>
        
            <form action="/add" method="GET">
            <input type="file" name="upload" class="form-control bootstrap-table-filter-control-name" id="formSelect1" />
         
            <div class="accordion" id="accordionPanelsStayOpen">
                <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-heading1"><span class="accordion-button"
                            type="button"
                            aria-expanded="true" aria-controls="panelsStayOpen-collapse1">Filterform somewhere</button>
                    </h2>
                    <div id="panelsStayOpen-collapse1" class="accordion-collapse collapse show"
                        aria-labelledby="panelsStayOpen-heading1">
                        <div class="accordion-body">
                            
                       


                        <input type="text" name="df" class="form-control bootstrap-table-filter-control-name" id="formSelect1"
                            placeholder="displayName" />
                          
                    </div>
                </div>
            </div>

            </div>

            <input type="submit" value="add">
        
        </form>
            </div>
        
        <h2>
        Dfs Comparison
        
        </h2>
        <% if(df.length >0){ %>
            <% for (let ia=0; ia < df.length;ia++ ) { %>
            
             
                <table id="table" class="table table-striped " data-filter-control="true" data-show-search-clear-button="true"
                    data-filter-control-container="#search">
                    <thead>
                        <tr>
            
                            
                            <th data-field="id">agent-id</th>
            
                           
                            <th data-field="price" data-filter-control="select">displayName</th>
                          

                        </tr>
                    </thead>
                    <tbody>
                 
                         
                                    <tr>
                                       
                                        <td>
                                            <%= files[ia].id %>
                                        
                                        </td>
                                        <td>
                                          <%= df[ia] %>
            
                                        </td>
                                  
            
                                    </tr>
                             
                              
            
                    </tbody>
                </table>
                    <% } %>
                    <% } %>
                 

        <label for="upload" class="form-label">choose parameters</label>
        <form action="/compare" method="GET">
            <input type="text" name="parameters" class="form-control bootstrap-table-filter-control-name" id="formSelect1" />
        
            <input type="submit" value="compare">
        </form>
      
        <% if(param.length>0){ %>
      
        
        
                <table id="table" class="table table-striped " data-filter-control="true" data-show-search-clear-button="true"
                    data-filter-control-container="#search">
                    <thead>
                        <tr>
        
        
                            <th data-field="id">displayName</th>
        
        <th data-field="price" data-filter-control="select">parameter</th>
                            <th data-field="price" data-filter-control="select">data</th>
        
        
                        </tr>
                    </thead>
                    <tbody>
        <% if(files.length > 0){ %>
        <% for(let i =0 ;i< files.length;i++  ){ %>
                        <tr>
                            
                            <td>
                                <%= df[i] %>
        
                            </td>
                                <% if(param.length>2) { %>
                         <td>
                            <%=  parameter[i] %>
                         </td>
                            <td>

                            
                             
                                <%= files[i][param[0]][0][param[1]][i][param[2]]%>
                             
                                    <!-- <form action="/update" method="GET">
                                        <input type="text" name="update" id="">
                                    </form>
                                 -->
                                    </td>
                            
                           
                        <% } else { %>
                        <td>
                           
                        
                        </td>
            <%} %>
                        </tr> 
        
        
            <% } %>
                <% } %>
                    </tbody>
           
                </table>
               
                <% } %>
                <h2>copy intents or entities</h2>
    <div class="accordion" id="accordionPanelsStayOpen">
        <div class="accordion-item">
    
          
    
            <div class="mb-3">
    
    
                <form action="/copy" method="GET">
                    <input type="file" name="upload" class="form-control bootstrap-table-filter-control-name"
                        id="formSelect2" />
                    <input type="file" name="upload2" class="form-control bootstrap-table-filter-control-name"
                        id="formSelect3"  multiple/>
    
    
                    <input type="submit" value="copy">
    
                </form>
            </div>
            <!-- <form action="/copy" method="GET">
    
        <input type="text" name="parameters" class="form-control bootstrap-table-filter-control-name" id="formSelect1" />
    
        <input type="submit" value="compare">
    </form> -->
    
        </div>
    </div>
        </div>

    
</body>
<script src="https://code.jquery.com/jquery-3.6.3.min.js"
    integrity="sha256-pvPw+upLPUjgMXY0G+8O0xUf+/Im1MZjXxxgOcBQBXU=" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.min.js"
    integrity="sha384-+sLIOodYLS7CIrQpBjl+C7nPvqq+FbNUBDunl/OZv93DB7Ln/533i8e/mZXLi/P+"
    crossorigin="anonymous"></script>
<script>
    $(function () {
        $('#table').bootstrapTable()
    })
</script>

</html>