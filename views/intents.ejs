 <div class="container">

<form id="search" method="GET" action="/customize">
    <div class="accordion" id="accordionPanelsStayOpen">
        <div class="accordion-item">
            <h2 class="accordion-header" id="panelsStayOpen-heading1"><span class="accordion-button" type="button"
                    data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapse1" aria-expanded="true"
                    aria-controls="panelsStayOpen-collapse1">Filterform somewhere</button></h2>
            <div id="panelsStayOpen-collapse1" class="accordion-collapse collapse show"
                aria-labelledby="panelsStayOpen-heading1">
                <div class="accordion-body">
                    <select name="oversoft-35450" id="">
                        <% for (let i=0; i< intents.length;i++ ) { %>
                        
                            <option value=<%= intents[1]%> ><%= intents[1]%> </option>
                    <option value=<%=intents[1]%> ><%= intents[1]%>
                    </option>
                <option value=<%=intents[1]%> ><%= intents[1]%>
                </option>
                         %>
                        <% } %>
                    </select>
                    <div class="mb-3"><label for="grbuchstabe" class="form-label">intent</label><input
                            class="form-control bootstrap-table-filter-control-name" id="grbuchstabe" name="intents"></input></div>
                    <div class="mb-3"><label for="grbuchstabe" class="form-label" name="entities">entities</label><input
                           name="entities" class="form-control bootstrap-table-filter-control-name" id="grbuchstabe"></input></div>
                    <!-- <div class="mb-3"><label for="datensatzkennung" class="form-label">Price</label><input
                            class="form-control bootstrap-table-filter-control-price" id="datensatzkennung"></input>
                    </div> -->
                </div>
            </div>
        </div>
    </div>
<input type="submit" value="submit">
</form>
<table id="table" class="table table-striped "  data-filter-control="true" data-show-search-clear-button="true"
    data-filter-control-container="#search">
    <thead>
        <tr>
            <th data-field="id">ID</th>
            <th data-field="name" data-filter-control="input"> Name</th>
            <th data-field="price" data-filter-control="select">display name</th>
        </tr>
    </thead>
    <tbody>
        <% for (let i=0; i< intents.length;i++ ) { %>   
            <% for (const intent of intents) { %>
                <tr>
                    <th scope="row"><% i %></th>
                    <td>
                        <%= intents[i].name %>
                    </td>
                </tr>
                <% } %>
                    <% } %>
        
        
    </tbody>
</table>
</div>
